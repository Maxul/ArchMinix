// cstdarg standard header

 #if !defined(_CSTDARG_) && defined(__MWERKS__)	/* compiler test */
 #define _CSTDARG_
 #include <va_list.h>
 #include <stdarg.win32.h>
 #endif /* __MWERKS__ */

#ifndef _CSTDARG_
#define _CSTDARG_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
 #define _CPP_STYLE_HEADER     /* for __TI_COMPILER_VERSION__ */
  #include <stdarg.h>
 #define _STD_USING
 #undef _CPP_STYLE_HEADER      /* for __TI_COMPILER_VERSION__ */

#else /* _STD_USING */
 #include <stdarg.h>

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD va_list;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _STD_USING */
#endif /* _CSTDARG_ */

